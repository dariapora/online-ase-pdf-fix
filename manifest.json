{
  "manifest_version": 3,
  "name": "online.ase.ro PDF New Tab Fix",
  "version": "1.0",
  "description": "Forces PDF links on online.ase.ro to open in a new tab.",
  "content_scripts": [
    {
      "matches": ["*://online.ase.ro/*"],
      "js": ["content.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "fixpdf@daria.ro"
    }
  }
}